{
  "expo": {
    "name": "EmCasa Storybook",
    "description": "",
    "slug": "storybook-native",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ignoreNodeModulesValidation": true,
    "packagerOpts": {
      "config": "metro.config.js"
    },
    "githubUrl": "https://github.com/emcasa/ui/"
  }
}
